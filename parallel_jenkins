pipeline{
	agent any 
	
	tools{
		maven 'mymaven'
	}
	
	
	stages{
		stage('Parallel'){
			parallel{
				stage('test the code'){
					steps{
						git 'https://github.com/krutharthkj/Jenkinsfile-Demo-pipleineCode.git'
						sh 'mvn test'
					}				
				}
				
				stage('code review'){
					steps{
						sh 'mvn pmd:pmd'
					}					
				}
				
				
			}
		}
		
		
		
		
	}
}

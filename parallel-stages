pipeline{

agent any 

tools{
  maven 'mymaven'
}

stages{

  stage('Parallel Stages'){
    parallel {
         stage('Compile Code'){
                  steps{
                    git 'https://github.com/Sonal0409/DevOpsCodeDemo.git'
                    sh 'mvn compile'

                      }
                  }

         stage('Review the Code'){
                  steps{
                  
                     sh 'mvn pmd:pmd'

                       }
                }
}
  }
}

}
